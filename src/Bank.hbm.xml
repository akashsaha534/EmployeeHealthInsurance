<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>
<class name="beans.Bank" table="Bank_Details">
		<id column="EmpId" name="EmpId" type="java.lang.Integer" />
		<property column="AccNo" name="AccNo" type="java.lang.Long" />
		<property column="BankName" name="BankName" type="java.lang.String" />
		<property column="IFSC" name="IFSC" type="java.lang.String" />
		<property column="Is_Deleted" name="Is_Deleted" type="java.lang.Character"/>		
	</class>
</hibernate-mapping>